#include <iostream>
#include "ant.hpp"
#include "colony.hpp"
#include "instance.hpp"


using namespace std;

int main(int argc, char *argv[])
{
		if (argc >= 2 )
		{
				cout << "Reading Isntance File" << endl;
				Instance problem = Instance::GetInstanceFromFile(argv[1]);
				cout << problem << endl;
				getchar();

				Colony colony = Colony::GetInstance();
				cout << colony << endl;
				
		}
  
  return 0;
}
