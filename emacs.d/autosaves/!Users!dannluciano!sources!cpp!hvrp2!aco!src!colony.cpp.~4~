#include "colony.hpp"

using namespace std;

Colony::Colony(Instance& problem) : problem(problem) {
		this->number_of_ants = this->problem.GetNumberCustomers();
		this->ants = vector<Ant>(this->number_of_ants);
		size_t i;
		for (i = 0; i < this->ants.size(); ++i) {
				this->ants[i] = Ant();
		}
}

Colony::~Colony()
{
}
